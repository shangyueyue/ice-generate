{
  "name": "ice-generate",
  "version": "1.0.1",
  "description": "just for ice ui generate",
  "author": "shangyueyue",
  "license": "MIT",
  "bin": {
    "ice-generate": "bin/index.js"
  },
  "main": "bin/index.js",
  "homepage": "https://github.com/shangyueyue/ice-generate",
  "repository": "github:shangyueyue/ice-generate",
  "bugs": "https://github.com/shangyueyue/ice-generate/issues",
  "keywords": [
    "ice-generate"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "lint": "eslint --ext .js bin --fix",
    "prettier": "prettier --write bin/**/*.js",
    "commit": "git-cz"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^2.20.0",
    "ejs": "^2.6.1",
    "fs-extra": "^8.1.0",
    "inquirer": "^7.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-eslint": "^10.0.1",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.0.2",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.2.1",
    "lint-staged": "^8.1.0",
    "prettier": "^1.17.0"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint src --ext .js --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
