
import React from 'react';
import { FormattedMessage } from 'react-intl';

function T(id) {
  return (<FormattedMessage id={id} />);
}

export default function panelconfigfn(data={}) {
  return [<%for(var i=0;i<panelConfigData.length;i++){ var item=panelConfigData[i]; var body = item.body; %>
          {
            header: T('<%=item.header%>'),
            model: '<%=item.model%>',
            body: [<%for(var j=0;j<body.length;j++){%>
                {
                  key: '<%=body[j].key%>',
                  label: T('<%=body[j].label%>'),
                  value: data.<%=body[j].value%>,
                }<%}%>
            ]
          },<%}%>
  ];
}
